{
  "$schema": "vscode://schemas/color-theme",
  "name": "Mintstream: Aero (Non-Italic)",
  "type": "dark",
  "colors": {
    "editor.background": "#161D26",
    "sideBar.background": "#12181F",
    "activityBar.background": "#12181F",
    "foreground": "#C5D1DE",
    "editor.foreground": "#C5D1DE",
    "focusBorder": "#00E5E5",
    "button.background": "#004D4D",
    "button.foreground": "#00E5E5",
    "button.hoverBackground": "#006B6B",
    "button.border": "#00E5E560",
    "activityBar.activeBorder": "#00E5E5",
    "activityBar.foreground": "#00E5E5",
    "activityBar.inactiveForeground": "#005050",
    "activityBarBadge.background": "#00E5E5",
    "activityBarBadge.foreground": "#12181F",
    "tab.activeBackground": "#161D26",
    "tab.inactiveBackground": "#12181F",
    "tab.activeBorder": "#00E5E5",
    "tab.activeForeground": "#E6F1F5",
    "tab.inactiveForeground": "#6C7A90",
    "tab.border": "#1A2836",
    "editor.selectionBackground": "#00E5E530",
    "editor.selectionHighlightBackground": "#00E5E520",
    "editor.lineHighlightBackground": "#1A283620",
    "editor.lineHighlightBorder": "#00E5E525",
    "editorLineNumber.foreground": "#3A4856",
    "editorLineNumber.activeForeground": "#6C8A9E",
    "editor.findMatchBackground": "#FF7F5080",
    "editor.findMatchHighlightBackground": "#FF7F5040",
    "editor.findMatchBorder": "#FF7F50",
    "list.activeSelectionBackground": "#004D4D",
    "list.activeSelectionForeground": "#00E5E5",
    "list.inactiveSelectionBackground": "#1A2836",
    "list.hoverBackground": "#1A283640",
    "list.highlightForeground": "#00E5E5",
    "statusBar.background": "#12181F",
    "statusBar.foreground": "#7D8A98",
    "statusBar.border": "#1A283650",
    "statusBar.debuggingBackground": "#004D4D",
    "statusBar.debuggingForeground": "#00E5E5",
    "statusBarItem.remoteBackground": "#004D4D",
    "statusBarItem.remoteForeground": "#00E5E5",
    "errorForeground": "#FF5555",
    "editorError.foreground": "#FF5555",
    "editorWarning.foreground": "#FF7F50",
    "editorGutter.modifiedBackground": "#FF7F5080",
    "editorGutter.addedBackground": "#00E5E580",
    "editorGutter.deletedBackground": "#FF555580",
    "terminal.ansiGreen": "#00E5E5",
    "terminal.ansiYellow": "#FF7F50",
    "terminal.ansiRed": "#FF5555",
    "terminal.ansiBlue": "#58A6FF",
    "terminal.ansiCyan": "#00E5E5",
    "terminal.ansiMagenta": "#C792EA",
    "terminal.ansiWhite": "#C5D1DE",
    "terminal.ansiBrightGreen": "#1AFFF3",
    "panel.background": "#12181F",
    "panelTitle.activeForeground": "#00E5E5",
    "panel.border": "#1A283650",
    "editorBracketMatch.background": "#00E5E520",
    "editorBracketMatch.border": "#00E5E5",
    "scrollbarSlider.background": "#00E5E530",
    "scrollbarSlider.hoverBackground": "#00E5E550",
    "scrollbarSlider.activeBackground": "#00E5E580",
    "editorWidget.background": "#12181F",
    "editorWidget.border": "#1A2836",
    "editorSuggestWidget.background": "#12181F",
    "editorSuggestWidget.border": "#1A2836",
    "editorSuggestWidget.selectedBackground": "#004D4D",
    "editorSuggestWidget.highlightForeground": "#00E5E5",
    "editorHoverWidget.background": "#12181F",
    "editorHoverWidget.border": "#1A2836",
    "editorHoverWidget.statusBarBackground": "#161D26",
    "input.background": "#1A2836",
    "input.border": "#00E5E560",
    "input.placeholderForeground": "#3A4856",
    "inputOption.activeBackground": "#004D4D",
    "inputOption.activeForeground": "#00E5E5",
    "dropdown.background": "#1A2836",
    "dropdown.border": "#00E5E560",
    "button.secondaryBackground": "#1A2836",
    "button.secondaryForeground": "#C5D1DE",
    "checkbox.background": "#1A2836",
    "checkbox.border": "#00E5E560",
    "checkbox.foreground": "#00E5E5",
    "peekView.border": "#00E5E5",
    "peekViewEditor.background": "#161D26",
    "peekViewResult.background": "#12181F",
    "peekViewResult.selectionBackground": "#004D4D",
    "peekViewTitle.background": "#12181F",
    "notificationCenterHeader.background": "#12181F",
    "notifications.background": "#1A2836",
    "notifications.border": "#1A2836",
    "debugToolBar.background": "#12181F",
    "settings.headerForeground": "#00E5E5",
    "settings.textInputBackground": "#1A2836",
    "settings.focusedRowBackground": "#1A283620",
    "settings.focusedRowBorder": "#00E5E5",
    "list.focusBackground": "#1A283640",
    "list.focusForeground": "#C5D1DE",
    "badge.background": "#00E5E5",
    "badge.foreground": "#12181F",
    "progressBar.background": "#00E5E5"
  },
  "tokenColors": [
    {
      "name": "Structure (Keywords, Operators, this/super)",
      "scope": [
        "keyword.control",
        "storage.type",
        "storage.modifier",
        "keyword.operator",
        "variable.language.this",
        "variable.language.super"
      ],
      "settings": { "foreground": "#9DAABF" }
    },
    {
      "name": "Action (Functions, Components)",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "entity.name.class",
        "support.class.component",
        "variable.other.constant.object"
      ],
      "settings": { "foreground": "#00E5E5", "fontStyle": "bold" }
    },
    {
      "name": "Data (Numbers, Constants)",
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": { "foreground": "#FF7F50" }
    },
    {
      "name": "Strings (De-emphasized)",
      "scope": ["string", "punctuation.definition.string"],
      "settings": { "foreground": "#A0B3CC" }
    },
    {
      "name": "Variables & Parameters",
      "scope": ["variable.other.readwrite", "variable"],
      "settings": { "foreground": "#C5D1DE" }
    },
    {
      "name": "Parameters",
      "scope": ["variable.parameter"],
      "settings": { "foreground": "#C5D1DE" }
    },
    {
      "name": "Muted (Tags, Object Keys *Definition*)",
      "scope": [
        "entity.name.tag",
        "meta.object-literal.key",
        "string.unquoted.label.js",
        "variable.other.object.key.js"
      ],
      "settings": { "foreground": "#6C7A90" }
    },
    {
      "name": "Object Property Access",
      "scope": [
        "variable.object.property",
        "support.variable.property",
        "variable.other.object.property"
      ],
      "settings": { "foreground": "#C5D1DE" }
    },
    {
      "name": "Types (TS/JS)",
      "scope": ["entity.name.type", "support.type", "support.class"],
      "settings": { "foreground": "#58A6FF" }
    },
    {
      "name": "TypeScript Type Keywords (as, satisfies)",
      "scope": ["keyword.operator.type", "storage.modifier.satisfies"],
      "settings": { "foreground": "#9DAABF" }
    },
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#657B8A" }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#FF5555", "fontStyle": "bold underline" }
    }
  ]
}
